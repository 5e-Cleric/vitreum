{
  "name": "vitreum",
  "version": "5.0.9",
  "description": "Web app build system using Browserify, React, and Less",
  "bin": "lib/vitreum",
  "repository": {
    "type": "git",
    "url": "git://github.com/stolksdorf/vitreum.git"
  },
  "scripts": {
    "test:init": "mkdir test_proj && cd ./test_proj && npm init -y && npm link ../ && vitreum init -y",
    "test": "pico-check tests/**/*.test.js",
    "dev": "pico-check -v -w tests/**/*.test.js"
  },
  "author": "Scott Tolksdorf <scott.tolksdorf@gmail.com> (https://github.com/stolksdorf)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stolksdorf/vitreum/issues"
  },
  "vitreum": {
    "targets": [],
    "babel": {
      "presets": [
        "@babel/preset-stage-3",
        "@babel/preset-react"
      ]
    }
  },
  "homepage": "https://github.com/stolksdorf/vitreum",
  "dependencies": {
    "@babel/core": "^7.0.0-beta.40",
    "@babel/preset-react": "^7.0.0-beta.40",
    "@babel/preset-stage-3": "^7.0.0-beta.40",
    "browserify": "^14.4.0",
    "chalk": "^2.0.1",
    "create-react-class": "^15.6.3",
    "fs-extra": "^3.0.1",
    "js-yaml": "^3.10.0",
    "less": "^2.7.1",
    "livereload": "^0.6.0",
    "lodash": "^4.17.5",
    "marked": "^0.3.19",
    "nodemon": "^1.11.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "source-map-support": "^0.4.6",
    "through2": "^2.0.3",
    "uglifyify": "^4.0.5",
    "watchify": "^3.8.0",
    "yargs": "^11.0.0"
  },
  "keywords": [
    "browserify",
    "react",
    "build"
  ],
  "devDependencies": {
    "pico-check": "^1.0.4"
  }
}
